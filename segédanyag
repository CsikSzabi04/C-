#include <iostream>
#include <sstream>
#include <string>
#include <fstream>
#include <vector>
using namespace std;

struct A {
    string els;  
    string mas;   
};

int main() {
    setlocale(LC_ALL, "hun");
    srand((unsigned int)time(0));

    // file megnyit√°sa
    fstream file("valaszok.txt");
    if (!file.is_open()) {cout << "Nem nyilik meg :(\n";return 1;}

    string sor;
    int s = 0;
    vector<A> list;

       
	while (getline(file, sor)) {
        if (s == 0) {s++; continue;}  // first line out

		istringstream iss(sor);
		A v;
		iss >> v.p0 >> v.p1 >> v.v0 >> v.v1 >> v.pl0 >> v.pl1;

		list.push_back(v);
	}

    for (const A& s : list) {cout << s.els << " - " << s.mas << "\n"; }
    
    ofstream ki("fgh.txt");
    for (const A& t : list) {ki << t.els << "-" << t.mas << "\n";}
    

    return 0;
}


